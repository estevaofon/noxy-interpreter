use sys select *
use rand select *
use time select *

let letters: string = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
let size: int = length(letters)
let i: int = 12
let password: string = ""

let rng: RandomGenerator = RandomGenerator(now(), 1664525, 1013904223, 2147483647)
while i > 0 do
    let r2: int = random_int(rng, 1, size)
    let index: int = r2 - 1
    password = password + letters[index]
    i = i - 1
end
print(f"Password: {password}")