use sys select *

let letters: string = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
let size: int = length(letters)
let i: int = 12
let password: string = ""
while i > 0 do
    let r2: SysResult = exec_output(f"powershell -Command \"Get-Random -Minimum 0 -Maximum {size} \"")
    let index: int = to_int(r2.output)
    password = password + letters[index]
    i = i - 1
end
print(f"Password: {password}")